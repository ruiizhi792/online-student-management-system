		if(itemData){
		  this.sitlzbp = {...itemData}
		  this.tt18271 = true
		}else if(this.gsus89d.length > 0){
		  this.sitlzbp = {...this.gsus89d[0]}
		  this.tt18271 = true
		}else{
		  this.$Message.error('请选择需要修改的数据项')
		}
		this.$refs.editFormData.resetFields();
	  },
	

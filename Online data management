	amomarz() {
		},

		w8b96dc(name){
		  this.$refs[name].validate((valid) => {
			if (valid) {
			  trainHoursService.reset(this.sitlzbp).then(res => {
				if(res.success){
				  this.$Message.success('修改成功')
				  this.rq3m3xa()
				  this.qzfosjh()
				}else{
				  this.$Message.error('修改失败')
				}
			  })
			} else {
				this.$Message.error('请填写完整');
			}
		  })
		},
		kws1sus(id){
			let ids = []
			if(id){
				ids.push(id)
			}else if(this.gsus89d.length > 0){
				ids = this.gsus89d.map(item => item.id)
			}else{
				this.$Message.error('请选择删除数据')
			}
			trainHoursService.deleteTrainHours(ids).then(res => {
			if(res.success){
					this.gsus89d = []
					this.$Message.success('删除成功')
					this.qzfosjh()
				} else {
					this.$Message.error('删除失败')
				}
			})
		},
		fpyrlrh(name){
		  this.$refs[name].validate((valid) => {
			if (valid) {
			  trainHoursService.insertTrainHours(this.sitlzbp).then(res => {
				if(res.success){
				  this.$Message.success('新增成功')
				  this.u2x51sy()
				  this.qzfosjh()
				}else{
				  this.$Message.error('新增失败')
				}
			  })
			} else {
				this.$Message.error('请填写完整');
			}
		  })
		},
		  qzfosjh() {
			trainHoursService.getTrainHoursPageList(this.hqsnmb6Data, this.n7turgd.j_pageNum, this.n7turgd.j_pageSize).then(res => {
			  if(res.success){
				this.yzw2ltb = res.data
			  }
			})
		  },

	  qyacfe7(){
		this.qb5jzj6 = true
		this.$refs.addFormData.resetFields();
	  },
	
	  u2x51sy(){
		this.qb5jzj6 = false
		this.sitlzbp = {
		  'trainingAndTeachingPlan':'',
		  'trainingCourseName':'',
		  'trainingCoursePlan':'',
		  'trainingCourseContent':'',
		  'courseTrainingTime':'',
		  'trainingPurpose':'',
		}
	  },
	
	  z5wlodb(itemData){
		if(itemData){
		  this.sitlzbp = {...itemData}
		  this.tt18271 = true
		}else if(this.gsus89d.length > 0){
		  this.sitlzbp = {...this.gsus89d[0]}
		  this.tt18271 = true
		}else{
		  this.$Message.error('请选择需要修改的数据项')
		}

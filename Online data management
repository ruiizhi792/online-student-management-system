	amomarz() {
			if (valid) {
			  trainingRecordsService.reset(this.sitlzbp).then(res => {
				if(res.success){
				  this.$Message.success('修改成功')
				  this.rq3m3xa()
				  this.qzfosjh()
				}else{
				  this.$Message.error('修改失败')

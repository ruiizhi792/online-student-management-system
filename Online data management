			  if(res.success){
				this.yzw2ltb = res.data
			  }
			})
		  },

	  qyacfe7(){
		this.qb5jzj6 = true
		this.$refs.addFormData.resetFields();
	  },
	
	  u2x51sy(){
		this.qb5jzj6 = false
		this.sitlzbp = {
		  'trainingCourse':'',
		  'trainers':'',
		  'trainingProgramme':'',
		  'trainingCycle':'',
		  'trainingEffect':'',
		  'peiXunRiQi':'',
		}
	  },
	

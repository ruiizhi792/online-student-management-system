<script>
import {
trainingPlanManagementService} from '@/api'
export default {
data(){
	return{
	  pz2jpin: {
	  },
	  n7turgd:{
	  },
	  mj36oy9:'',
	  nc1cfgg:{
			'planScheme':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'trainingTeachers':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'trainingTime':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'trainingObjectives':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'trainingCourseContent':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'planning':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
	  },
		  m764pqh: {
		  },
	  hqsnmb6Data:{},
	  yzw2ltb:{
		list:[]
	  },
		  vq4bq5c: {
		  },
	  gsus89d:[],
	  tt18271:false,
	  qb5jzj6:false,
	  sitlzbp:{
		'planScheme':'',
		'trainingTeachers':'',
		'trainingTime':'',
		'trainingObjectives':'',
		'trainingCourseContent':'',
		'planning':'',
	  },
	}
},
mounted () {
	this.mj36oy9 = sessionStorage.getItem('sMenuName') || sessionStorage.getItem('fMenuName');
	this.qzfosjh();
	this.amomarz();
},
methods:{
	amomarz() {
		},

		kws1sus(id){
			let ids = []
			if(id){
				ids.push(id)
			}else if(this.gsus89d.length > 0){
				ids = this.gsus89d.map(item => item.id)
			}else{
				this.$Message.error('请选择删除数据')
			}
			trainingPlanManagementService.deleteTrainingPlanManagement(ids).then(res => {
			if(res.success){
					this.gsus89d = []
					this.$Message.success('删除成功')
					this.qzfosjh()
				} else {
					this.$Message.error('删除失败')
				}
			})
		},
		w8b96dc(name){
		  this.$refs[name].validate((valid) => {
			if (valid) {
			  trainingPlanManagementService.reset(this.sitlzbp).then(res => {
				if(res.success){
				  this.$Message.success('修改成功')
				  this.rq3m3xa()
				  this.qzfosjh()
				}else{
				  this.$Message.error('修改失败')
				}
			  })
			} else {
				this.$Message.error('请填写完整');
			}
		  })
		},
		fpyrlrh(name){
		  this.$refs[name].validate((valid) => {
			if (valid) {
			  trainingPlanManagementService.insertTrainingPlanManagement(this.sitlzbp).then(res => {
				if(res.success){
				  this.$Message.success('新增成功')
				  this.u2x51sy()
				  this.qzfosjh()
				}else{
				  this.$Message.error('新增失败')
				}
			  })
			} else {
				this.$Message.error('请填写完整');
			}
		  })
		},
		  qzfosjh() {
			trainingPlanManagementService.getTrainingPlanManagementPageList(this.hqsnmb6Data, this.n7turgd.j_pageNum, this.n7turgd.j_pageSize).then(res => {
			  if(res.success){
				this.yzw2ltb = res.data
			  }
			})
		  },

	  qyacfe7(){
		this.qb5jzj6 = true
		this.$refs.addFormData.resetFields();
	  },
	
	  u2x51sy(){
		this.qb5jzj6 = false
		this.sitlzbp = {
		  'planScheme':'',
		  'trainingTeachers':'',
		  'trainingTime':'',
		  'trainingObjectives':'',
		  'trainingCourseContent':'',
		  'planning':'',
		}
	  },
	
	  z5wlodb(itemData){
		if(itemData){
		  this.sitlzbp = {...itemData}
		  this.tt18271 = true
		}else if(this.gsus89d.length > 0){
		  this.sitlzbp = {...this.gsus89d[0]}
		  this.tt18271 = true
		}else{
		  this.$Message.error('请选择需要修改的数据项')
		}
		this.$refs.editFormData.resetFields();
	  },
	
	  rq3m3xa(){
		this.tt18271 = false
		this.sitlzbp = {
		  'planScheme':'',
		  'trainingTeachers':'',
		  'trainingTime':'',
		  'trainingObjectives':'',
		  'trainingCourseContent':'',
		  'planning':'',
		}
	  },
	
	  hqsnmb6(){
		this.n7turgd.j_pageNum = 1
		this.qzfosjh()
	  },
	
	  bidgnm0(){
		if(this.yzw2ltb.hasPreviousPage){
		  this.n7turgd.j_pageNum = this.yzw2ltb.prePage
		  this.qzfosjh()
		}
		
	  },
	
	  z3l8ha8(){
		if(this.yzw2ltb.hasNextPage){
		this.n7turgd.j_pageNum = this.yzw2ltb.nextPage
		this.qzfosjh()
		}
	  },

	i8e9ru4(e){
	  let checked = e.target.checked
	  let checkDom = this.$refs.table.getElementsByClassName('selBtn')
	  if(checked){
		this.gsus89d = []
		for(let i=0; i<checkDom.length; i++){
		  checkDom[i].checked = true
		  this.gsus89d.push(checkDom[i]._value)
		}
	  }else{
		for(let i=0; i<checkDom.length; i++){
		  checkDom[i].checked = false
		}
		this.gsus89d = []
	  }
	},

	eiqnc23(e){
	  let checked = e.target.checked
	  let val = e.target._value
	  if(checked){
		this.gsus89d.push(val)
	  }else{
		let clearIndex = this.gsus89d.indexOf(val)
		this.gsus89d.splice(clearIndex,1)
	  }
	},

	dl6v5r7(e){
	  let val = e.target._value
	  this.gsus89d = []
	  this.gsus89d.push(val)
	},

	l85582s(pageNumb){
	  this.n7turgd.j_pageNum = pageNumb
	  this.qzfosjh()
	}
}
}
</script>


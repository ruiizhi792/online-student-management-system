<script>
import {
userInformationService} from '@/api'
export default {
data(){
	return{
	  pz2jpin: {
	  },
	  gsus89d:[],
	  n7turgd:{
	  },
	  hqsnmb6Data:{},
	  qb5jzj6:false,
	  nc1cfgg:{
			'trainer':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'age':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'telphone':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'trainingPurpose':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'trainingEffect':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'learningInformation':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
	  },
	  yzw2ltb:{
		list:[]
	  },
		  m764pqh: {
		  },
	  sitlzbp:{
		'trainer':'',
		'age':'',
		'telphone':'',
		'trainingPurpose':'',
		'trainingEffect':'',
		'learningInformation':'',
	  },
	  tt18271:false,
		  vq4bq5c: {
		  },
	}
},
mounted () {
	this.qzfosjh();
	this.amomarz();
},
methods:{
	amomarz() {
		},

		  qzfosjh() {
			userInformationService.getUserInformationPageList(this.hqsnmb6Data, this.n7turgd.j_pageNum, this.n7turgd.j_pageSize).then(res => {
			  if(res.success){
				this.yzw2ltb = res.data
			  }
			})
		  },
		kws1sus(id){
			let ids = []
			if(id){
				ids.push(id)
			}else if(this.gsus89d.length > 0){
				ids = this.gsus89d.map(item => item.id)
			}else{
				this.$Message.error('请选择删除数据')
			}
			userInformationService.deleteUserInformation(ids).then(res => {
			if(res.success){
					this.gsus89d = []
					this.$Message.success('删除成功')
					this.qzfosjh()

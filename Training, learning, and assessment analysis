				this.$Message.error('请填写完整');
			}
		  })
		},
		  vvjpte2() {
			trainingPlanService.getList(this.ezbaliiData, this.bwvil74.ia5cmb4, this.bwvil74.i9p05z8).then(res => {
			  if(res.success){
				this.xzo5si4 = res.data
			  }
			})
		  },
		j_editData(name){
		  this.$refs[name].validate((valid) => {
			if (valid) {
			  trainingPlanService.modifyTrainingPlan(this.ntqm713).then(res => {
				if(res.success){
				  this.$Message.success('修改成功')
				  this.nm7qf3c()
				  this.vvjpte2()
				}else{
				  this.$Message.error('修改失败')
				}
			  })
			} else {
				this.$Message.error('请填写完整');
			}
		  })
		},
	
	  resypan(){
		this.izu6vui = true
		this.$refs.addFormData.resetFields();
	  },
	  ydvj4kt(){
		this.izu6vui = false
		this.ntqm713 = {
		  'trainingAndTeachingPlan':'',
		  'trainingSteps':'',
		  'trainingProgressControl':'',
		  'producer':'',
		  'trainingObjectives':'',

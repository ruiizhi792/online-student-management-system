<script>
import {
workingAbilityIndexService} from '@/api'
export default {
data(){
	return{
	  i30w687:[],
	  tjmsnvb: {
	  },
	  ahr9wkq: {
	  },
	  izu6vui:false,
	  bwvil74:{
		ia5cmb4:1,
		i9p05z8:10,
	  },
	  s8ec05l:false,
	  xzo5si4:{
		list:[]
	  },
	  ntqm713:{
		'trainHours':'',
		'trainingProgramme':'',
		'nameTrainProgram':'',
		'peiXunJiangShi':'',
		'trainContent':'',
		'trainees':'',
	  },
	  khpab28:{
			'trainHours':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'trainingProgramme':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'nameTrainProgram':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'peiXunJiangShi':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'trainContent':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'trainees':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
	  },
	  ktsx070: {
	  },
	  ezbaliiData:{},
	}
},
mounted () {
	this.vvjpte2()
	this.q55uzs9();
},
methods:{
	
	q55uzs9() {
		},
		awxswaf(id){
				let ids = []
				if(id){
					ids.push(id)
				}else if(this.i30w687.length > 0){
					ids = this.i30w687.map(item => item.id)
				}
				workingAbilityIndexService.deleteWorkingAbilityIndex(ids).then(res => {
				if(res.success){
						this.i30w687 = []
						this.$Message.success('删除成功')
						this.vvjpte2()
					} else {
						this.$Message.error('删除失败')
					}
				})
			},
		  vvjpte2() {
			workingAbilityIndexService.getList(this.ezbaliiData, this.bwvil74.ia5cmb4, this.bwvil74.i9p05z8).then(res => {
			  if(res.success){
				this.xzo5si4 = res.data
			  }
			})
		  },
		j_editData(name){
		  this.$refs[name].validate((valid) => {
			if (valid) {
			  workingAbilityIndexService.modifyWorkingAbilityIndex(this.ntqm713).then(res => {
				if(res.success){
				  this.$Message.success('修改成功')
				  this.nm7qf3c()
				  this.vvjpte2()
				}else{
				  this.$Message.error('修改失败')
				}
			  })
			} else {
				this.$Message.error('请填写完整');
			}
		  })
		},
	
		ph51itf(name){
		  this.$refs[name].validate((valid) => {
			if (valid) {
			  workingAbilityIndexService.addWorkingAbilityIndex(this.ntqm713).then(res => {
				if(res.success){
				  this.$Message.success('新增成功')
				  this.ydvj4kt()
				  this.vvjpte2()
				}else{
				  this.$Message.error('新增失败')
				}
			  })
			} else {
				this.$Message.error('请填写完整');
			}
		  })
		},
	  resypan(){
		this.izu6vui = true
		this.$refs.addFormData.resetFields();
	  },
	  ydvj4kt(){
		this.izu6vui = false
		this.ntqm713 = {
		  'trainHours':'',
		  'trainingProgramme':'',
		  'nameTrainProgram':'',
		  'peiXunJiangShi':'',
		  'trainContent':'',
		  'trainees':'',
		}
	  },
	  hlrpgwf(itemData){
		if(itemData){
		  this.ntqm713 = {...itemData}
		  this.s8ec05l = true
		}else if(this.i30w687.length > 0){
		  this.ntqm713 = {...this.i30w687[0]}
		  this.s8ec05l = true
		}else{
		  this.$Message.error('请选择需要修改的数据项')
		}
		this.$refs.editFormData.resetFields();
	  },
	  nm7qf3c(){
		this.s8ec05l = false
		this.ntqm713 = {
		  'trainHours':'',
		  'trainingProgramme':'',
		  'nameTrainProgram':'',
		  'peiXunJiangShi':'',
		  'trainContent':'',
		  'trainees':'',
		}
	  },
	  ezbalii(){
		this.bwvil74.ia5cmb4 = 1
		this.vvjpte2()
	  },
	  wd3jfn7(){
		if(this.xzo5si4.hasPreviousPage){
		  this.bwvil74.ia5cmb4 = this.xzo5si4.prePage
		  this.vvjpte2()
		}
		
	  },
	  e2wo6ij(){
		if(this.xzo5si4.hasNextPage){
		this.bwvil74.ia5cmb4 = this.xzo5si4.nextPage
		this.vvjpte2()
		}
	  },
	khbzfkm(e){
	  let checked = e.target.checked
	  let checkDom = this.$refs.table.getElementsByClassName('b66jw8mat')
	  if(checked){
		this.i30w687 = []
		for(let i=0; i<checkDom.length; i++){
		  checkDom[i].checked = true
		  this.i30w687.push(checkDom[i]._value)
		}
	  }else{
		for(let i=0; i<checkDom.length; i++){
		  checkDom[i].checked = false
		}
		this.i30w687 = []
	  }
	  console.log(this.i30w687)
	},
	f8crt0w(e){
	  let checked = e.target.checked
	  let val = e.target._value
	  if(checked){
		this.i30w687.push(val)
	  }else{
		let clearIndex = this.i30w687.indexOf(val)
		this.i30w687.splice(clearIndex,1)
	  }
	},
	lsuueja(e){
	  let val = e.target._value
	  this.i30w687 = []
	  this.i30w687.push(val)
	},
	bpxny01(pageNumb){
	  this.bwvil74.ia5cmb4 = pageNumb
	  this.vvjpte2()
	}
}
}
</script>


	q55uzs9() {
		},
		awxswaf(id){
				let ids = []
				if(id){
					ids.push(id)
				}else if(this.i30w687.length > 0){
					ids = this.i30w687.map(item => item.id)
				}
				trainingPlanService.deleteTrainingPlan(ids).then(res => {
				if(res.success){
						this.i30w687 = []
						this.$Message.success('删除成功')
						this.vvjpte2()
					} else {
						this.$Message.error('删除失败')
					}
				})
			},
		ph51itf(name){
		  this.$refs[name].validate((valid) => {
			if (valid) {
			  trainingPlanService.addTrainingPlan(this.ntqm713).then(res => {
				if(res.success){
				  this.$Message.success('新增成功')
				  this.ydvj4kt()

		kh34r9q(name){
		  this.$refs[name].validate((valid) => {
			if (valid) {
			  practicalTrainingOfTrainingSkillsService.modifyPracticalTrainingOfTrainingSkills(this.ntqm713).then(res => {
				if(res.success){
				  this.$Message.success('修改成功')
				  this.nm7qf3c()
				  this.vvjpte2()
				}else{
				  this.$Message.error('修改失败')
				}
			  })
			} else {
				this.$Message.error('请填写完整');
			}
		  })
		},
		awxswaf(id){
			let ids = []
			if(id){
				ids.push(id)
			}else if(this.i30w687.length > 0){
				ids = this.i30w687.map(item => item.id)
			}else{
				this.$Message.error('请选择删除数据')
			}
			practicalTrainingOfTrainingSkillsService.deletePracticalTrainingOfTrainingSkills(ids).then(res => {
			if(res.success){
					this.i30w687 = []
					this.$Message.success('删除成功')
					this.vvjpte2()
				} else {
					this.$Message.error('删除失败')
				}
			})
		},

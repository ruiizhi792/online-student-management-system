				}else{
				  this.$Message.error('新增失败')
				}
			  })
			} else {
				this.$Message.error('请填写完整');
			}
		  })
		},
		  vvjpte2() {
			practicalTrainingOfTrainingSkillsService.getList(this.ezbaliiData, this.bwvil74.j_pageNum, this.bwvil74.j_pageSize).then(res => {
			  if(res.success){
				this.xzo5si4 = res.data
			  }
			})
		  },

	  resypan(){
		this.izu6vui = true
		this.$refs.addFormData.resetFields();
	  },
	
	  ydvj4kt(){
		this.izu6vui = false
		this.ntqm713 = {
		  'practicalTrainingCourses':'',

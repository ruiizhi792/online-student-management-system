	/**
	* 培训时间
	*/
	@Column(name = "nameTrainProgram")
	private String nameTrainProgram;

	/**
	* 培训讲师
	*/
	@Column(name = "peiXunJiangShi")
	private String peiXunJiangShi;

	/**
	* 培训内容
	*/
	@Column(name = "trainContent")
	private String trainContent;

	/**
	* 受培训人员
	*/
	@Column(name = "trainees")
	private String trainees;
}

			hrxr172:false,
			ixrl2nj:false,
		}
	},
	mounted () {
		this.i8xqtjl = sessionStorage.getItem('sMenuName') || sessionStorage.getItem('fMenuName')
		this.gtdpiuk()
		this.a8b0zio();
	},
	methods: {
		a8b0zio() {
		},

			gjuru9n(id){
				let ids = []
				if(id){
					ids.push(id)
				}else if(this.pvcoel9.length > 0){
					ids = this.pvcoel9.map(item => item.id)
				}
				learningMaterialManagementService.remove(ids).then(res => {
				if(res.success){
						this.pvcoel9 = []
						this.$Message.success('删除成功')
						this.gtdpiuk()
					} else {
						this.$Message.error('删除失败')
					}
				})
			},
			u20pjrp(name){
			this.$refs[name].validate((valid) => {
				if (valid) {
				learningMaterialManagementService.change(this.jnr5jcf).then(res => {
					if(res.success){
					this.$Message.success('修改成功')
					this.u3sm9g1()
					this.gtdpiuk()
					}else{
					this.$Message.error('修改失败')
					}
				})
				} else {
					this.$Message.error('请填写完整');
				}
			})
			},
		
			cdz4wfn(name){
			this.$refs[name].validate((valid) => {
				if (valid) {
				learningMaterialManagementService.addLearningMaterialManagement(this.jnr5jcf).then(res => {
					if(res.success){
					this.$Message.success('新增成功')
					this.x95t9ci()
					this.gtdpiuk()
					}else{
					this.$Message.error('新增失败')
					}
				})
				} else {
					this.$Message.error('请填写完整');
				}
			})
			},
			gtdpiuk() {
				learningMaterialManagementService.getPage(this.dlbuo18Data, this.qjjzz66.c401f4j, this.qjjzz66.evhh4ge).then(res => {
				if(res.success){
					this.v9mlsd2 = res.data
				}
				})
			},
		//开启新增
		udodr5u(){
			this.hrxr172 = true
			this.$refs.addFormData.resetFields();
		},
		//关闭新增
		x95t9ci(){
			this.hrxr172 = false
			this.jnr5jcf = {
			'nameOfData':'',
			'documentNo':'',
			'dataStatus':'',
			'descriptionOfData':'',
			'dataType':'',
			'governanceMeasures':'',
			}
		},
		//开启修改
		n8il5ch(itemData){
			if(itemData){
			this.jnr5jcf = {...itemData}
			this.ixrl2nj = true
			}else if(this.pvcoel9.length > 0){
			this.jnr5jcf = {...this.pvcoel9[0]}
			this.ixrl2nj = true
			}else{
			this.$Message.error('请选择需要修改的数据项')
			}
			this.$refs.editFormData.resetFields();
		},
		//关闭修改
		u3sm9g1(){
			this.ixrl2nj = false
			this.jnr5jcf = {
			'nameOfData':'',
			'documentNo':'',
			'dataStatus':'',
			'descriptionOfData':'',
			'dataType':'',
			'governanceMeasures':'',
			}
		},
		//搜索
		dlbuo18(){
			this.qjjzz66.c401f4j = 1
			this.gtdpiuk()
		},
		// 全选返选
		nd2kknu(e){
		let checked = e.target.checked
		let checkDom = this.$refs.table.getElementsByClassName('q5uf76gu0')
		if(checked){
			this.pvcoel9 = []
			for(let i=0; i<checkDom.length; i++){

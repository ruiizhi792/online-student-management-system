<script>
import {
jiaoCaiGuanLiService} from '@/api'
export default {
data(){
	return{
	  hrxr172:false,
	  tq089iy: {
	  },
	  d1lh1wk: {
	  },
	  k8rz7gm: {
	  },
	  ixrl2nj:false,
	  vqusmmq:{
			'nameOfTextbook':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'textbookCode':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'categoryOfTeachingMaterials':[
			  {
				required: true,
				message: '不能为空'
			  }

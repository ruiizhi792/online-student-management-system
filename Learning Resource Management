				required: true,
				message: '不能为空'
			  }
			],
	  },
	  v9mlsd2:{
		list:[]
	  },
	  jnr5jcf:{
		'nameOfTextbook':'',
		'textbookCode':'',
		'categoryOfTeachingMaterials':'',
		'publishHouse':'',
		'usageRecord':'',
		'usageTime':'',
	  },
	  dlbuo18Data:{},
	  qjjzz66:{
		evhh4ge:10,
		c401f4j:1,
	  },
	  pvcoel9:[],
	}
},
mounted () {
	this.gtdpiuk()
	this.a8b0zio();
},
methods:{
	
	a8b0zio() {
		},
		  gtdpiuk() {
			jiaoCaiGuanLiService.getPage(this.dlbuo18Data, this.qjjzz66.c401f4j, this.qjjzz66.evhh4ge).then(res => {
			  if(res.success){
				this.v9mlsd2 = res.data
			  }
			})
		  },
		cdz4wfn(name){
		  this.$refs[name].validate((valid) => {
			if (valid) {
			  jiaoCaiGuanLiService.addJiaoCaiGuanLi(this.jnr5jcf).then(res => {
				if(res.success){
				  this.$Message.success('新增成功')
				  this.x95t9ci()
				  this.gtdpiuk()
				}else{
				  this.$Message.error('新增失败')
				}
			  })
			} else {
				this.$Message.error('请填写完整');
			}
		  })
		},
		gjuru9n(id){
				let ids = []
				if(id){
					ids.push(id)
				}else if(this.pvcoel9.length > 0){
					ids = this.pvcoel9.map(item => item.id)
				}
				jiaoCaiGuanLiService.remove(ids).then(res => {
				if(res.success){
						this.pvcoel9 = []
						this.$Message.success('删除成功')
						this.gtdpiuk()
					} else {
						this.$Message.error('删除失败')
					}
				})
			},
		j_editData(name){
		  this.$refs[name].validate((valid) => {
			if (valid) {
			  jiaoCaiGuanLiService.change(this.jnr5jcf).then(res => {
				if(res.success){
				  this.$Message.success('修改成功')
				  this.u3sm9g1()
				  this.gtdpiuk()
				}else{
				  this.$Message.error('修改失败')
				}
			  })
			} else {
				this.$Message.error('请填写完整');
			}
		  })
		},
	
	  udodr5u(){
		this.hrxr172 = true
		this.$refs.addFormData.resetFields();
	  },
	  x95t9ci(){
		this.hrxr172 = false
		this.jnr5jcf = {
		  'nameOfTextbook':'',
		  'textbookCode':'',
		  'categoryOfTeachingMaterials':'',
		  'publishHouse':'',
		  'usageRecord':'',
		  'usageTime':'',
		}
	  },
	  n8il5ch(itemData){
		if(itemData){
		  this.jnr5jcf = {...itemData}
		  this.ixrl2nj = true
		}else if(this.pvcoel9.length > 0){
		  this.jnr5jcf = {...this.pvcoel9[0]}
		  this.ixrl2nj = true
		}else{
		  this.$Message.error('请选择需要修改的数据项')
		}
		this.$refs.editFormData.resetFields();
	  },
	  u3sm9g1(){
		this.ixrl2nj = false
		this.jnr5jcf = {
		  'nameOfTextbook':'',
		  'textbookCode':'',
		  'categoryOfTeachingMaterials':'',
		  'publishHouse':'',
		  'usageRecord':'',
		  'usageTime':'',
		}
	  },
	  dlbuo18(){
		this.qjjzz66.c401f4j = 1
		this.gtdpiuk()
	  },
	  gcri7no(){
		if(this.v9mlsd2.hasPreviousPage){
		  this.qjjzz66.c401f4j = this.v9mlsd2.prePage
		  this.gtdpiuk()
		}
		
	  },
	  gdlh9r0(){
		if(this.v9mlsd2.hasNextPage){
		this.qjjzz66.c401f4j = this.v9mlsd2.nextPage
		this.gtdpiuk()
		}
	  },
	nd2kknu(e){
	  let checked = e.target.checked
	  let checkDom = this.$refs.table.getElementsByClassName('q5uf76gu0')
	  if(checked){
		this.pvcoel9 = []
		for(let i=0; i<checkDom.length; i++){
		  checkDom[i].checked = true
		  this.pvcoel9.push(checkDom[i]._value)
		}
	  }else{
		for(let i=0; i<checkDom.length; i++){
		  checkDom[i].checked = false
		}
		this.pvcoel9 = []
	  }
	  console.log(this.pvcoel9)
	},
	a9lhmhm(e){
	  let checked = e.target.checked
	  let val = e.target._value
	  if(checked){
		this.pvcoel9.push(val)
	  }else{
		let clearIndex = this.pvcoel9.indexOf(val)
		this.pvcoel9.splice(clearIndex,1)
	  }
	},
	whh2eon(e){
	  let val = e.target._value
	  this.pvcoel9 = []
	  this.pvcoel9.push(val)
	},
	i89uv7m(pageNumb){
	  this.qjjzz66.c401f4j = pageNumb
	  this.gtdpiuk()
	}
}
}

<script>
import {
accountManagementService} from '@/api'
export default {
data(){
	return{
	  dlbuo18Data:{},
	  vqusmmq:{
			'accountName':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'accountCode':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'classTime':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'keynoteTeacher':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'accountCategory':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'subjectEvaluation':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
	  },
	  hrxr172:false,
		i8xqtjl:'',
		  tq089iy: {
		  },
	  jnr5jcf:{
		'accountName':'',
		'accountCode':'',
		'classTime':'',
		'keynoteTeacher':'',
		'accountCategory':'',
		'subjectEvaluation':'',
	  },
		  d1lh1wk: {
		  },
	  qjjzz66:{
	  },
	  k8rz7gm: {
	  },
	  v9mlsd2:{
		list:[]
	  },
	  pvcoel9:[],
	  ixrl2nj:false,
	}
},
mounted () {
	this.i8xqtjl = sessionStorage.getItem('sMenuName') || sessionStorage.getItem('fMenuName')
	this.gtdpiuk();
	this.a8b0zio();
},
methods:{
	a8b0zio() {
		},
		gjuru9n(id){
			let ids = []
			if(id){
				ids.push(id)
			}else if(this.pvcoel9.length > 0){
				ids = this.pvcoel9.map(item => item.id)
			}else{
				this.$Message.error('请选择删除数据')
			}
			accountManagementService.remove(ids).then(res => {
			if(res.success){
					this.pvcoel9 = []
					this.$Message.success('删除成功')
					this.gtdpiuk()
				} else {
					this.$Message.error('删除失败')
				}
			})
		},
		u20pjrp(name){
		  this.$refs[name].validate((valid) => {
			if (valid) {
			  accountManagementService.change(this.jnr5jcf).then(res => {
				if(res.success){
				  this.$Message.success('修改成功')
				  this.u3sm9g1()
				  this.gtdpiuk()
				}else{
				  this.$Message.error('修改失败')
				}
			  })
			} else {
				this.$Message.error('请填写完整');
			}
		  })
		},
	
		cdz4wfn(name){
		  this.$refs[name].validate((valid) => {
			if (valid) {
			  accountManagementService.addAccountManagement(this.jnr5jcf).then(res => {
				if(res.success){
				  this.$Message.success('新增成功')
				  this.x95t9ci()
				  this.gtdpiuk()
				}else{
				  this.$Message.error('新增失败')
				}
			  })
			} else {
				this.$Message.error('请填写完整');
			}
		  })
		},
		  gtdpiuk() {
			accountManagementService.getPage(this.dlbuo18Data, this.qjjzz66.j_pageNum, this.qjjzz66.j_pageSize).then(res => {
			  if(res.success){
				this.v9mlsd2 = res.data
			  }
			})
		  },

	  udodr5u(){
		this.hrxr172 = true
		this.$refs.addFormData.resetFields();
	  },
	  x95t9ci(){
		this.hrxr172 = false
		this.jnr5jcf = {
		  'accountName':'',
		  'accountCode':'',
		  'classTime':'',
		  'keynoteTeacher':'',
		  'accountCategory':'',
		  'subjectEvaluation':'',
		}
	  },
	
	  n8il5ch(itemData){
		if(itemData){
		  this.jnr5jcf = {...itemData}
		  this.ixrl2nj = true
		}else if(this.pvcoel9.length > 0){
		  this.jnr5jcf = {...this.pvcoel9[0]}
		  this.ixrl2nj = true
		}else{
		  this.$Message.error('请选择需要修改的数据项')
		}
		this.$refs.editFormData.resetFields();
	  },
	
	  u3sm9g1(){
		this.ixrl2nj = false
		this.jnr5jcf = {
		  'accountName':'',
		  'accountCode':'',
		  'classTime':'',
		  'keynoteTeacher':'',
		  'accountCategory':'',
		  'subjectEvaluation':'',
		}
	  },
	
	  dlbuo18(){
		this.qjjzz66.j_pageNum = 1
		this.gtdpiuk()
	  },
	
	  gcri7no(){
		if(this.v9mlsd2.hasPreviousPage){
		  this.qjjzz66.j_pageNum = this.v9mlsd2.prePage
		  this.gtdpiuk()
		}
		
	  },
	
	  gdlh9r0(){

<script>
import {
consultingRecordManagementService} from '@/api'
export default {
data(){
	return{
	  k8rz7gm: {
	  },
	  v9mlsd2:{
		list:[]
	  },
	  qjjzz66:{
	  },
	  hrxr172:false,
	  dlbuo18Data:{},
	  vqusmmq:{
			'consultingNo':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'consultingCustomers':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'contactInformation':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'customServiceNu':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'typeOfConsultation':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'consultationSatisfaction':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
	  },
		  tq089iy: {
		  },
	  jnr5jcf:{
		'consultingNo':'',
		'consultingCustomers':'',
		'contactInformation':'',
		'customServiceNu':'',
		'typeOfConsultation':'',
		'consultationSatisfaction':'',
	  },
	  ixrl2nj:false,
		  d1lh1wk: {
		  },
		i8xqtjl:'',
	  pvcoel9:[],
	}
},
mounted () {
	this.i8xqtjl = sessionStorage.getItem('sMenuName') || sessionStorage.getItem('fMenuName')
	this.gtdpiuk();
	this.a8b0zio();
},
methods:{
	a8b0zio() {
		},
		gjuru9n(id){
			let ids = []
			if(id){
				ids.push(id)
			}else if(this.pvcoel9.length > 0){
				ids = this.pvcoel9.map(item => item.id)
			}else{
				this.$Message.error('请选择删除数据')
			}
			consultingRecordManagementService.remove(ids).then(res => {
			if(res.success){
					this.pvcoel9 = []
					this.$Message.success('删除成功')
					this.gtdpiuk()
				} else {
					this.$Message.error('删除失败')
				}
			})
		},
		  gtdpiuk() {
			consultingRecordManagementService.getPage(this.dlbuo18Data, this.qjjzz66.j_pageNum, this.qjjzz66.j_pageSize).then(res => {
			  if(res.success){
				this.v9mlsd2 = res.data
			  }
			})
		  },
		cdz4wfn(name){
		  this.$refs[name].validate((valid) => {
			if (valid) {
			  consultingRecordManagementService.addConsultingRecordManagement(this.jnr5jcf).then(res => {
				if(res.success){
				  this.$Message.success('新增成功')
				  this.x95t9ci()
				  this.gtdpiuk()
				}else{
				  this.$Message.error('新增失败')
				}
			  })
			} else {
				this.$Message.error('请填写完整');
			}
		  })
		},
		u20pjrp(name){
		  this.$refs[name].validate((valid) => {
			if (valid) {
			  consultingRecordManagementService.change(this.jnr5jcf).then(res => {
				if(res.success){
				  this.$Message.success('修改成功')
				  this.u3sm9g1()
				  this.gtdpiuk()
				}else{
				  this.$Message.error('修改失败')
				}
			  })
			} else {
				this.$Message.error('请填写完整');
			}
		  })
		},
	

	  udodr5u(){
		this.hrxr172 = true
		this.$refs.addFormData.resetFields();
	  },
	  x95t9ci(){
		this.hrxr172 = false
		this.jnr5jcf = {
		  'consultingNo':'',
		  'consultingCustomers':'',
		  'contactInformation':'',
		  'customServiceNu':'',
		  'typeOfConsultation':'',
		  'consultationSatisfaction':'',
		}
	  },
	
	  n8il5ch(itemData){
		if(itemData){
		  this.jnr5jcf = {...itemData}
		  this.ixrl2nj = true
		}else if(this.pvcoel9.length > 0){
		  this.jnr5jcf = {...this.pvcoel9[0]}
		  this.ixrl2nj = true
		}else{
		  this.$Message.error('请选择需要修改的数据项')
		}
		this.$refs.editFormData.resetFields();
	  },
	
	  u3sm9g1(){
		this.ixrl2nj = false
		this.jnr5jcf = {
		  'consultingNo':'',
		  'consultingCustomers':'',
		  'contactInformation':'',
		  'customServiceNu':'',
		  'typeOfConsultation':'',
		  'consultationSatisfaction':'',
		}
	  },
	
	  dlbuo18(){
		this.qjjzz66.j_pageNum = 1
		this.gtdpiuk()
	  },
	
	  gcri7no(){
		if(this.v9mlsd2.hasPreviousPage){
		  this.qjjzz66.j_pageNum = this.v9mlsd2.prePage
		  this.gtdpiuk()
		}
		
	  },
	
	  gdlh9r0(){
		if(this.v9mlsd2.hasNextPage){
		this.qjjzz66.j_pageNum = this.v9mlsd2.nextPage
		this.gtdpiuk()
		}
	  },

	nd2kknu(e){
	  let checked = e.target.checked
	  let checkDom = this.$refs.table.getElementsByClassName('selBtn')
	  if(checked){
		this.pvcoel9 = []
		for(let i=0; i<checkDom.length; i++){
		  checkDom[i].checked = true
		  this.pvcoel9.push(checkDom[i]._value)
		}
	  }else{
		for(let i=0; i<checkDom.length; i++){
		  checkDom[i].checked = false
		}
		this.pvcoel9 = []
	  }
	},
	
	a9lhmhm(e){
	  let checked = e.target.checked
	  let val = e.target._value
	  if(checked){
		this.pvcoel9.push(val)
	  }else{
		let clearIndex = this.pvcoel9.indexOf(val)
		this.pvcoel9.splice(clearIndex,1)
	  }
	},
	
	whh2eon(e){
	  let val = e.target._value
	  this.pvcoel9 = []
	  this.pvcoel9.push(val)
	},

	i89uv7m(pageNumb){
	  this.qjjzz66.j_pageNum = pageNumb
	  this.gtdpiuk()
	}

<script>
import {
monitoringManagementService} from '@/api'
export default {
data(){
	return{
		  ee0gqn8: {
		  },
	  f06agzc: {
			  'operationStatus': '正常 异常'.split(" "),
	  },
	  vjrb66h:false,
	  zcpbdhi:{
		'robotName':'',
		'monitorDetail':'',
		'operationStatus':'',
		'detectDepth':'',
		'runTime':'',
		'headDepart':'',
	  },
	  d752hda:false,
	  whmlun3:{
			'robotName':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'monitorDetail':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'operationStatus':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'detectDepth':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'runTime':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
			'headDepart':[
			  {
				required: true,
				message: '不能为空'
			  }
			],
	  },
		  o7ujlkc: {
		  },
	  xwzg8an:{
		list:[]
	  },
	  e9pzkft:[],
	  jpjesdt:{
	  },
	  zow3pf0Data:{},
	}
},
mounted () {
	this.jqjukz8();
	this.phz2x98();
},
methods:{
	phz2x98() {
		},

		  jqjukz8() {
			monitoringManagementService.getList(this.zow3pf0Data, this.jpjesdt.j_pageNum, this.jpjesdt.j_pageSize).then(res => {
			  if(res.success){
				this.xwzg8an = res.data
			  }
			})
		  },
		oajtnbq(name){
		  this.$refs[name].validate((valid) => {
			if (valid) {
			  monitoringManagementService.add(this.zcpbdhi).then(res => {
				if(res.success){
				  this.$Message.success('新增成功')
				  this.wu36myv()
				  this.jqjukz8()
				}else{
				  this.$Message.error('新增失败')
				}
			  })
			} else {
				this.$Message.error('请填写完整');
			}
		  })
		},
		x2x5zrx(name){
		  this.$refs[name].validate((valid) => {
			if (valid) {
			  monitoringManagementService.changeMonitoringManagement(this.zcpbdhi).then(res => {
				if(res.success){
				  this.$Message.success('修改成功')
				  this.p8ec4lr()
				  this.jqjukz8()
				}else{
				  this.$Message.error('修改失败')
				}
			  })
			} else {
				this.$Message.error('请填写完整');
			}
		  })
		},
		lqbrxhn(id){
			let ids = []
			if(id){
				ids.push(id)
			}else if(this.e9pzkft.length > 0){
				ids = this.e9pzkft.map(item => item.id)
			}else{
				this.$Message.error('请选择删除数据')
			}
			monitoringManagementService.removeMonitoringManagement(ids).then(res => {
			if(res.success){
					this.e9pzkft = []
					this.$Message.success('删除成功')
					this.jqjukz8()
				} else {
					this.$Message.error('删除失败')
				}
			})
		},

	  b3c83ek(){
		this.vjrb66h = true
		this.$refs.addFormData.resetFields();
	  },
	
	  wu36myv(){
		this.vjrb66h = false
		this.zcpbdhi = {
		  'robotName':'',
		  'monitorDetail':'',
		  'operationStatus':'',
		  'detectDepth':'',
		  'runTime':'',
		  'headDepart':'',
		}
	  },
	
	  p3i4cu0(itemData){
		if(itemData){
		  this.zcpbdhi = {...itemData}
		  this.d752hda = true
		}else if(this.e9pzkft.length > 0){
		  this.zcpbdhi = {...this.e9pzkft[0]}
		  this.d752hda = true
		}else{
		  this.$Message.error('请选择需要修改的数据项')
		}
		this.$refs.editFormData.resetFields();
	  },
	
	  p8ec4lr(){
		this.d752hda = false
		this.zcpbdhi = {
		  'robotName':'',
		  'monitorDetail':'',
		  'operationStatus':'',
		  'detectDepth':'',
		  'runTime':'',
		  'headDepart':'',
		}
	  },
	
	  zow3pf0(){
		this.jpjesdt.j_pageNum = 1
		this.jqjukz8()
	  },
	
	  m9fopqw(){
		if(this.xwzg8an.hasPreviousPage){
		  this.jpjesdt.j_pageNum = this.xwzg8an.prePage
		  this.jqjukz8()
		}
		
	  },
	
	  tqckx6i(){
		if(this.xwzg8an.hasNextPage){
		this.jpjesdt.j_pageNum = this.xwzg8an.nextPage
		this.jqjukz8()
		}
	  },

	u5mj8p5(e){
	  let checked = e.target.checked
	  let checkDom = this.$refs.table.getElementsByClassName('selBtn')
	  if(checked){
		this.e9pzkft = []
		for(let i=0; i<checkDom.length; i++){
		  checkDom[i].checked = true
		  this.e9pzkft.push(checkDom[i]._value)
		}
	  }else{
		for(let i=0; i<checkDom.length; i++){
		  checkDom[i].checked = false
		}
		this.e9pzkft = []
	  }
	},

	so5t7mt(e){
	  let checked = e.target.checked
	  let val = e.target._value
	  if(checked){
		this.e9pzkft.push(val)
	  }else{
		let clearIndex = this.e9pzkft.indexOf(val)
		this.e9pzkft.splice(clearIndex,1)
	  }
	},

	wnv9gz0(e){
	  let val = e.target._value
	  this.e9pzkft = []
	  this.e9pzkft.push(val)
	},

	s0a2qzz(pageNumb){
	  this.jpjesdt.j_pageNum = pageNumb
	  this.jqjukz8()
	}
}
}
</script>


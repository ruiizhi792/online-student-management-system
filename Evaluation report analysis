			  if(res.success){
				this.xwzg8an = res.data
			  }
			})
		  },
		lqbrxhn(id){
			let ids = []
			if(id){
				ids.push(id)
			}else if(this.e9pzkft.length > 0){
				ids = this.e9pzkft.map(item => item.id)
			}else{
				this.$Message.error('请选择删除数据')
			}
			studentManagementService.removeStudentManagement(ids).then(res => {
			if(res.success){
					this.e9pzkft = []
					this.$Message.success('删除成功')
					this.jqjukz8()
				} else {
					this.$Message.error('删除失败')
				}
			})
		},
		x2x5zrx(name){
		  this.$refs[name].validate((valid) => {
			if (valid) {
			  studentManagementService.changeStudentManagement(this.zcpbdhi).then(res => {
				if(res.success){
				  this.$Message.success('修改成功')
				  this.p8ec4lr()
				  this.jqjukz8()
				}else{
				  this.$Message.error('修改失败')
				}
			  })
			} else {
				this.$Message.error('请填写完整');
			}
		  })
		},
		oajtnbq(name){
		  this.$refs[name].validate((valid) => {
			if (valid) {
			  studentManagementService.add(this.zcpbdhi).then(res => {
				if(res.success){
				  this.$Message.success('新增成功')
				  this.wu36myv()
				  this.jqjukz8()
				}else{
				  this.$Message.error('新增失败')
				}
			  })
			} else {
				this.$Message.error('请填写完整');

	phz2x98() {
		},

		  jqjukz8() {
			instructResourceService.getList(this.zow3pf0Data, this.jpjesdt.j_pageNum, this.jpjesdt.j_pageSize).then(res => {
			  if(res.success){
				this.xwzg8an = res.data
			  }
			})
		  },
		x2x5zrx(name){
		  this.$refs[name].validate((valid) => {
			if (valid) {
			  instructResourceService.changeInstructResource(this.zcpbdhi).then(res => {
				if(res.success){
				  this.$Message.success('修改成功')
				  this.p8ec4lr()
				  this.jqjukz8()
				}else{
				  this.$Message.error('修改失败')
				}
			  })

		oajtnbq(name){
		  this.$refs[name].validate((valid) => {
			if (valid) {
			  analysisSchemeManagementService.add(this.zcpbdhi).then(res => {
				if(res.success){
				  this.$Message.success('新增成功')
				  this.wu36myv()
				  this.jqjukz8()
				}else{
				  this.$Message.error('新增失败')
				}
			  })
			} else {
				this.$Message.error('请填写完整');
			}
		  })
		},

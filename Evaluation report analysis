		  this.$refs[name].validate((valid) => {
			if (valid) {
			  instructResourceService.add(this.zcpbdhi).then(res => {
				if(res.success){
				  this.$Message.success('新增成功')
				  this.wu36myv()
				  this.jqjukz8()
				}else{
				  this.$Message.error('新增失败')
				}
			  })
			} else {
				this.$Message.error('请填写完整');
			}
		  })
		},
		lqbrxhn(id){
			let ids = []
			if(id){
				ids.push(id)
			}else if(this.e9pzkft.length > 0){

package com.kenneth.miller.evaluationreportmanagement.req.page;

/**
* 描述
* 分页参数
*
*/
public class PageInfoREQ {

	private Integer pageNum;

	private Integer pageSize;

	public Integer getPageNum() {
		return pageNum == null ? 1 : pageNum;
	}

	public void setPageNum(Integer pageNum) {
		this.pageNum = pageNum;
	}

	public Integer getPageSize() {
		return pageSize == null ? 10 : pageSize;
	}

	public void setPageSize(Integer pageSize) {
		this.pageSize = pageSize;
	}
}
package com.kenneth.miller.evaluationreportmanagement.dto;

import io.swagger.annotations.ApiModelProperty;
import lombok.Data;
import javax.persistence.Column;

@Data
public class EvaluationReportManagementInfoDTO {
private Long id;


	/**
	* 报告标题
	*/
	@Column(name = "reportTitle")
	private String reportTitle;

	/**
	* 报告编号
	*/
	@Column(name = "reportNumber")
	private String reportNumber;

	/**
	* 报告人
	*/
	@Column(name = "reporter")
	private String reporter;

	/**
	* 报告内容
	*/
	@Column(name = "reportContent")
	private String reportContent;

	/**
	* 报告时间
	*/
	@Column(name = "chuangJianShiJian")
	private String chuangJianShiJian;

	/**
	* 报告状态
	*/
	@Column(name = "reportStatus")
	private String reportStatus;


}
